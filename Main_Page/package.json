{
  "name": "step-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=10.1.0"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint ./*.js && node_modules/.bin/htmlhint ./*.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "ignore": [],
    "linters": {
      "./*.js": "eslint",
      "./*.html": "htmlhint"
    },
    "relative": true
  },
  "dependencies": {
},
"devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "htmlhint": "^0.11.0",
    "husky": "^1.1.2",
    "lint-staged": "^8.1.0"
  }
}
